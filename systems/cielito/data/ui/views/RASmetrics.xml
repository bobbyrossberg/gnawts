<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>RAS metrics</label>
  <row>
    <chart>
      <searchName>systemStateHours</searchName>
      <title>Hours per Day</title>
      <option name="drilldown">all</option>
      <option name="charting.seriesColors">[0x55a16a,0xe6ae00,0xd85d34]</option>
    </chart>
    <chart>
      <searchName>SMTTI</searchName>
      <title>System Mean Time to Interrupt (9.12.2); "must be > 200 hrs"</title>
      <option name="drilldown">all</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>EL</searchName>
      <title>System Effectiveness Level (9.12.3); "must be > 90%"</title>
      <option name="drilldown">all</option>
    </chart>
    <chart>
      <searchName>JMTTI</searchName>
      <title>Job Mean Time To Interrupt (9.12.1); "must be > 25 hrs"</title>
      <option name="drilldown">all</option>
    </chart>
  </row>
</dashboard>
